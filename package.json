    "cssmin": "^0.4.3",
    "grunt": "^1.3.0",
    "grunt-contrib-sass": "^2.0.0",
    "gulp-htmlmin": "^5.0.1",
    "ios-icon-list": "^1.1.0",
    "jquery": "^3.5.1",
    "less": "^3.12.2",
    "node": "^12.18.3",
    "node-sass": "^4.14.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "ruby": "^0.6.1",
    "shared": "^0.2.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
