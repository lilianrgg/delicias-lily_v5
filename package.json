{
  "name": "delicias-lily",
  "version": "1.0.0",
  "description": "paquete de configuracion",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir  dist/img",
    "usemin": "usemin index.html -d dist -o dist/index.html --htmlmin true && usemin nosotros.html -d dist --htmlmin -o  dist/nosotros.html && usemin contacto.html -d dist --htmlmin -o  dist/contacto.html  && usemin precios.html -d dist --htmlmin -o  dist/precios.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://lilianrgg@bitbucket.org/lilianrgg/delicias-lily.git"
  },
  "author": "liliangomez",
  "license": "ISC",
  "homepage": "https://bitbucket.org/lilianrgg/delicias-lily#readme",
  "devDependencies": {
    "del": "^5.1.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-clean": "^0.4.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-usemin": "^0.3.30",
    "har-validator": "^5.1.5",
    "htmlmin": "0.0.7",
    "http-proxy-middleware": "^1.0.4",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.5.4",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "concurrently": "^5.3.0",
    "connect-history-api-fallback": "^1.6.0",
